<mvc:View
	controllerName="workshop.overview.controller.App"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	displayBlock="true"
	height="100%">
	<f:FlexibleColumnLayout id="fcl" layout="TwoColumnsBeginExpanded">
		<f:beginColumnPages>
			<mvc:XMLView id="beginView" viewName="workshop.overview.view.Master"/>
		</f:beginColumnPages>
		<f:midColumnPages>
			<mvc:XMLView id="midView" viewName="workshop.overview.view.Detail"/>
		</f:midColumnPages>
	</f:FlexibleColumnLayout>
</mvc:View>